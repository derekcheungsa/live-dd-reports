  {% if stylesheet %}
     <style type="text/css">
     {{ stylesheet }}
     .tweet-list {
        width: 65%;
      }
      .tweet {
        box-sizing: border-box;
        width: 100%;
        padding: 1rem;
        border: 1px solid #e1e8ed;
      }
      /* use larger font sizes for improved readability on small screens */
      .tweet h3 {
        font-size: 1.0rem;
      }
      .tweet p {
        font-size: 1.0rem;
      }
      .tweet-header {
        display: flex;
        align-items: top;
        margin-left: 1.0rem;
        font-family: "Helvetica Neue", sans-serif;
      }
      .tweet-header img {
        vertical-align: middle;
        border-radius: 50%;
      }
      .tweet-header h3 {
        font-size: 1.50rem;
        font-weight: bold;
        margin-left: 1.0rem;
        display: inline-block;
        margin: 0;
        font-family: "Helvetica Neue", sans-serif;
      }
      .tweet-header p {
        font-size: 1.5rem;
        font-weight: bold;
        margin-left: 8px;
        display: inline-block;
        margin: 0;
        font-family: "Helvetica Neue", sans-serif;
      }
      /* use touch-friendly buttons */
      .tweet-footer a {
        display: inline-block;
        padding: 0.5rem 1rem;
        border-radius: 0.25rem;
        background-color: #1da1f2;
        color: white;
        text-decoration: none;
      }
      /* scale down media elements */
      .tweet-media img {
        max-width: 75%;
        height: auto;
        margin-left: 1.0rem;
      }
      /* display profile picture, user name, and screen name on the same line */
      .tweet-header img {
        vertical-align: top;
      }
      .tweet-header h3 {
        display: inline-block;
        font-size: 1.65rem;
        margin: 0.25rem;
        vertical-align: top
      }
      .tweet-header p {
        display: inline-block;
        margin: 0.25rem;
        font-size: 1.65rem;
        vertical-align: top
      }
      /* use Twitter's color scheme and typography */
      .tweet-header h3,
      .tweet-header p {
        color: #14171a;
      }
      .tweet-body p {
        font-size: 1.0rem;
        line-height: 1.5;
        padding: 0 16px;
        color: #657786;
      }
      .tweet-footer p {
        font-size: 1.0rem;
        font-weight: bold;
        margin-left: 1.0rem;
        color: #657786;
      }
      .tweet-footer a {
        margin-left: 1.0rem;
      }
     </style>
  {% endif %}
</head>

